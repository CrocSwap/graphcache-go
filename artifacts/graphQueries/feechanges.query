query($minTime: BigInt, $maxTime: BigInt, $orderDir: OrderDirection) {
  feeChanges(first: 1000, where:  { block_gte: $minTime, block_lte: $maxTime }, 
    orderBy: time, orderDirection: $orderDir) {
    id
    transactionHash
    callIndex
    block
    time
    pool {
      id
      base
      quote
      poolIdx
    }
    feeRate
  }
}
